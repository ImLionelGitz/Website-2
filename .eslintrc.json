{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["/*.js"],
    "rules": {
        "@typescript-eslint/space-before-function-paren": "off",
        "indent": ["error", 2],
        "eol-last": "off"
    }
}